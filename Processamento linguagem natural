      "source": [
        "#Criando uma fun√ß√£o chamada \"Score de Sentimento\" para determinar os sentimentos associados\n",
        "def Score_sentimento(frase):\n",
        "    l_sentimento = []\n",
        "    for p in frase:\n",
        "        l_sentimento.append(int(dic_palavra_polaridade.get(p, 0)))\n",
        "    score = sum(l_sentimento)\n",
        "    return score"
      ],
      "execution_count": null,
      "outputs": []
    },
